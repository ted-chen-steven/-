#include<iostream>
#include<string>

using namespace std;

int main() {

	string operators;
	int number1, number2;
    
    cin >> number1 >> operators >> number2;
	
    if (operators == "+") {
		cout << number1 + number2 << endl;
	}
	else if (operators == "-") {
		cout << number1 - number2 << endl;
	}
	else if (operators == "*") {
		cout << number1 * number2 << endl;
	}
	else if (operators == "/") {
		cout << number1 / number2 << endl;
	}
	else if (operators == "%") {
		cout << number1 % number2 << endl;
	}
	else {
		cout << "ERROR" << endl;
	};
    
	return 0;

};
